};

SERVANT_INDEX_BY_NAME = {};
SERVANT_NAME_BY_INDEX = {};
Object.keys(SERVANT_NAME).forEach((key, idx) => {
    sname = SERVANT_NAME[key];
    SERVANT_INDEX_BY_NAME[sname] = idx;
    SERVANT_NAME_BY_INDEX[idx] = sname;
});
Object.freeze(SERVANT_INDEX_BY_NAME);
Object.freeze(SERVANT_NAME_BY_INDEX);


function getKey(name, element) {
    return name + '_' + element.text;
}

var DICT_SERVANT = {};

class Servant {
    constructor(rank, name, element, type, img,
        skill_1, skill_2, skill_brand, passive,
        brand_default, brand_sequel,
        team_buff, rel_1, rel_2,
        note) {
        this.idx = Object.keys(DICT_SERVANT).length + 1;

        this.rank = rank;
        this.name = name;
        this.element = element;
        this.type = type;
        this.img = img;

        this.skill_1 = skill_1;
        this.skill_2 = skill_2;
        this.skill_brand = skill_brand;
        this.passive = passive;

        this.brand_default = brand_default;
        this.brand_sequel = brand_sequel;

        this.team_buff = team_buff;
        this.rel_1 = rel_1;
        this.rel_2 = rel_2;

        this.note = note;

    }

    summary() {
        var res = '${this.name} (${this.type})';
        console.log(res);
        return res;
    }
}


